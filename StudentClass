using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

abstract class Student
{
    public Student(string s_name)
    {
        name = s_name;
        state = "";
    }
    
    public string name;
    public string state;

    public void Relax() { state = state.Insert(state.Length, "Relax"); }
    public void Read() { state = state.Insert(state.Length, "Read"); }
    public void Write() { state = state.Insert(state.Length, "Write"); }
    public abstract void Study();
}

class GoodStudent: Student
{
    public GoodStudent(string s_name): base (s_name)
    {
        name = s_name;
        state = state.Insert(state.Length, "good");
    }
    public override void Study()
    {
        Read(); Write(); Read(); Write(); Relax();
    }
}

class BadStudent : Student
{
    public BadStudent(string s_name) : base(s_name)
    {
        name = s_name;
        state = state.Insert(state.Length, "bad");
    }
    public override void Study()
    {
        Relax(); Relax(); Relax(); Relax(); Read();
    }
}
class Group
{
    public Group(string name) { groupname = name; }

    public string groupname;
    public List<Student> grouplist = new List<Student>();

    public void AddStudent(Student st)
    {
        grouplist.Add(st);
    }
    public void GetInfo()
    {
        Console.WriteLine(groupname);
        foreach(Student st in grouplist)
            Console.WriteLine(st.name);
    }
    public void GetFullInfo()
    {
        Console.WriteLine(groupname);
        foreach (Student st in grouplist)
        { 
            Console.WriteLine(st.name);
            Console.WriteLine(st.state);
        }
    }

}
namespace StudentClass
{
    class Program
    {
        static void Main(string[] args)
        {
            Group g = new Group(Console.ReadLine());
            BadStudent bs = new BadStudent(Console.ReadLine());
            g.AddStudent(bs);
            bs.Study();
            g.GetInfo();
            g.GetFullInfo();
        }
    }
}
